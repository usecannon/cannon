  "name": "@usecannon/website",
  "version": "2.15.5",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "contentlayer build && next build",
    "clean": "rm -rf ./out ./.next",
    "analyze": "ANALYZE=true pnpm run build",
    "tsc:build": "tsc",
    "start": "next start",
    "lint": "next lint --fix",
    "serve": "serve out -c ../serve.json -p 3000 > /dev/null 2>&1 &",
    "test": "vitest",
    "e2e": "NEXT_PUBLIC_E2E_TESTING_MODE=true pnpm run build && pnpm run serve  && cypress open --e2e",
    "e2e:local": "NEXT_PUBLIC_E2E_TESTING_MODE=true cypress run --e2e",
    "e2e:local:spec": "NEXT_PUBLIC_E2E_TESTING_MODE=true cypress run --e2e --spec \"cypress/integration/Interact.feature\"",
    "e2e:headless": "NEXT_PUBLIC_E2E_TESTING_MODE=true pnpm run build && pnpm run serve && cypress run --e2e",
    "production:install": "pnpm i --frozen-lockfile --filter @usecannon/builder --filter @usecannon/cli --filter @usecannon/api --filter @usecannon/website",
    "production:build": "pnpm --filter @usecannon/builder --filter @usecannon/cli --filter @usecannon/api run build && pnpm --filter @usecannon/website run build",
    "component": "cypress open --component",
    "component:headless": "cypress run --component"
  },
