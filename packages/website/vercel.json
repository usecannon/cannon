{
  "rewrites": [
    {
      "source": "/packages/:name",
      "destination": "/packages/[name].html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "IMaFooBar",
          "value": "indeed"
        }
      ]
    }
  ],
  "framework": null,
  "buildCommand": "cd ../.. && npm run build && cd packages/website && next build",
  "outputDirectory": "out"
}
